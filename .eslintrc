{
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
      "ecmaVersion": 6
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "chrome": true
  },
  "rules": {
    "semi": [2, "always"],
    "no-eq-null": 2,
    "no-console": 0,
    "no-multi-spaces": 0,
    "max-len": [2, 120, 4, {
      "ignoreUrls": true
    }]
  }
}